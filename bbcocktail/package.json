{
  "name": "vite-kea-react-ts-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:kea\" \"npm run dev:vite\" -n KEA,VITE -c green,magenta",
    "dev:kea": "kea-typegen watch --write-paths",
    "dev:vite": "vite",
    "build": "tsc && vite build",
    "prettier": "prettier --write src",
    "preview": "vite preview"
  },
  "dependencies": {
    "fuse.js": "^7.1.0",
    "kea": "^3.0.2",
    "kea-loaders": "^3.0.0",
    "kea-router": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^7.4.0",
    "kea-test-utils": "^0.2.2",
    "kea-typegen": "^3.3.6",
    "postcss": "8.5.3",
    "postcss-nesting": "^13.0.1",
    "prettier": "2.8.8",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.2"
  }
}
